<?xml version="1.0" encoding="UTF-8"?>
<web-app version="2.5" xmlns="http://java.sun.com/xml/ns/javaee" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-app_2_5.xsd">
    <servlet>
        <servlet-name>insertVendedor</servlet-name>
        <servlet-class>controller.insertVendedor</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>ingresarv</servlet-name>
        <servlet-class>controller.ingresarv</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>showv</servlet-name>
        <servlet-class>controller.showv</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>ingresarCompra</servlet-name>
        <servlet-class>controller.ingresarCompra</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>addProducto</servlet-name>
        <servlet-class>controller.addProducto</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>editarProducto</servlet-name>
        <servlet-class>controller.editarProducto</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>editProducto</servlet-name>
        <servlet-class>controller.editProducto</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>editProductos</servlet-name>
        <servlet-class>controller.editProductos</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>eliminarProducto</servlet-name>
        <servlet-class>controller.eliminarProducto</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>insertVendedor</servlet-name>
        <url-pattern>/insertVendedor</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>ingresarv</servlet-name>
        <url-pattern>/ingresarv</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>showv</servlet-name>
        <url-pattern>/showv</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>ingresarCompra</servlet-name>
        <url-pattern>/ingresarCompra</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>addProducto</servlet-name>
        <url-pattern>/addProducto</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>editarProducto</servlet-name>
        <url-pattern>/editarProducto</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>editProducto</servlet-name>
        <url-pattern>/editProducto</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>editProductos</servlet-name>
        <url-pattern>/editProductos</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>eliminarProducto</servlet-name>
        <url-pattern>/eliminarProducto</url-pattern>
    </servlet-mapping>
    <session-config>
        <session-timeout>
            30
        </session-timeout>
    </session-config>
    <welcome-file-list>
        <welcome-file>Views/wellcome.jsp</welcome-file>
    </welcome-file-list>
</web-app>
